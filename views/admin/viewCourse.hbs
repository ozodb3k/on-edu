<img src="{{course.img}}" alt="{{course.img}}">
<h3>{{course.title}}</h3>
<p>{{course.description}}</p>
<p>Price: {{course.price}}$</p>
<p>Author: {{course.author}}</p>
<a href="/api/courses/delete/{{course._id}}" class="delete course_delete">Delete</a>
<a href="/api/courses/update/{{course._id}}" class="update course_update">Update</a>